{
    "eslint.packageManager": "yarn",
    // "sync.gist": "4c3864e4ed71a5666943cd4154d6bf50",
    // "sync.syncExtensions": true,
    "workbench.startupEditor": "newUntitledFile",
    "editor.wordWrap": "on",
    //customer color
    //end
    // "editor.cursorSmoothCaretAnimation": true,
    // "editor.suggest.preview": true,
    //bracket
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairsHorizontal": true,
    "editor.guides.highlightActiveIndentation": "always",
    "editor.guides.bracketPairs": true,
    "editor.matchBrackets": "always",
    "editor.autoClosingBrackets": "always",
    //end bracket
    // "editor.inlayHints.enabled": "onUnlessPressed",
    // "javascript.suggest.autoImports": true,
    // "typescript.suggest.autoImports": true,
    // "javascript.autoClosingTags": true,
    // "typescript.autoClosingTags": true,
    // "javascript.format.enable": true,
    // "javascript.suggest.completeJSDocs": true,
    // "typescript.suggest.completeJSDocs": true,
    // "javascript.suggestionActions.enabled": true,
    // "typescript.suggestionActions.enabled": true,
    // "javascript.inlayHints.parameterNames.enabled": "all",
    // "typescript.inlayHints.parameterNames.enabled": "all",
    // "javascript.inlayHints.variableTypes.enabled": true,
    // "javascript.inlayHints.propertyDeclarationTypes.enabled":true,
    // "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
    // "javascript.inlayHints.parameterTypes.enabled": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    //prettier
    // "prettier.trailingComma": "es5",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "prettier.singleQuote": true,
    //end
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features",
        "editor.formatOnSave": true
    },
    // end
    "css.enabledLanguages": [
        "html"
    ],
    "html-to-css-autocompletion.autocompletionFilesScope": "multi-root",
    "editor.formatOnPaste": true,
    "editor.formatOnType": false,
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace",
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    // "terminal.integrated.fontFamily": "Fira Code Light",
    "terminal.integrated.fontFamily": "Fira Code Light",
    "terminal.integrated.fontSize": 15,
    // "editor.fontFamily": "Operator Mono Lig",
    "editor.fontFamily": "Fira Code Light",
    "debug.console.fontFamily": "Fira Code Light",
    "debug.console.fontSize": 15,
    "editor.fontLigatures": true,
    // "editor.semanticHighlighting.enabled": false,
    "editor.fontSize": 14,
    // vsintellicode
    "editor.quickSuggestionsDelay": 1,
    // "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.wordBasedSuggestions": true,
    "editor.parameterHints.enabled": true,
    "editor.quickSuggestions": {
        "other": true,
        "comments": true,
        "strings": true
    },
    //end
    // "autoimport.autoComplete": true,
    // "autoimport.useSemiColon": true,
    // "editor.codeActionsOnSave": {
    // "source.organizeImports": true,
    // "addMissingImports": true,
    // "source.fixAll.tslint": true,
    // "source.fixAll.eslint": true
    // "source.fixAll": true,
    // "fixAll.eslint": true
    // },
    //emmet
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        // "typescript": "typescriptreact",
        "razor": "html",
        "plaintext": "pug",
        "vue-html": "html",
        "vue": "html"
    },
    "emmet.excludeLanguages": [
        "markdown"
    ],
    "emmet.triggerExpansionOnTab": true,
    "emmet.useInlineCompletions": true,
    "emmet.syntaxProfiles": {
        "html": {
            "attr_quotes": "single"
        },
        "jsx": {
            "self_closing_tag": true
        },
        "vue-html": "html",
        "vue": "html"
    },
    //end
    "workbench.iconTheme": "material-icon-theme",
    "html.format.indentInnerHtml": true,
    "html.format.extraLiners": "head, body, /html",
    "tabnine.experimentalAutoImports": true,
    "editor.letterSpacing": 0.5,
    "editor.tabSize": 4,
    "liveServer.settings.donotShowInfoMsg": true,
    "html.suggest.html5": true,
    "html.autoClosingTags": true,
    // "editor.linkedEditing": true,
    "workbench.localHistory.enabled": true,
    // "files.autoSaveDelay": 10000,
    "material-icon-theme.activeIconPack": "nest",
    "editor.cursorBlinking": "expand",
    "typescript.suggest.paths": true,
    "javascript.suggest.paths": true,
    "editor.inlineSuggest.enabled": true,
    "[ignore]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    // extensions code runner
    "code-runner.runInTerminal": true,
    "code-runner.clearPreviousOutput": true,
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[dockerfile]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": []
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.formatOnSave": true,
    /*
    turboConsoleLog
    */
    "turboConsoleLog.logFunction": "log",
    "turboConsoleLog.logMessagePrefix": "",
    "turboConsoleLog.includeFileNameAndLineNum": false,
    "turboConsoleLog.delimiterInsideMessage": "->",
    "turboConsoleLog.addSemicolonInTheEnd": true,
    "indentRainbow.colors": [
        "rgba(240, 128, 128,0.15)",
        "rgba(244, 151, 142,0.15)",
        "rgba(248, 173, 157, 0.15)",
        "rgba(251, 196, 171,0.15)",
        "rgba(255, 218, 185,0.15)",
        "rgba(255, 218, 185,0.1)",
        "rgba(255, 218, 185,0.05)",
        "rgba(255, 218, 185,0.025)"
    ],
    /*
    */
    // "editor.snippetSuggestions": "inline",
}